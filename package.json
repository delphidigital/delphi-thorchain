{
  "name": "delphi-thorchain",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "fetch": "DATA_SOURCE=api node --experimental-modules scripts/saveApiResponses.mjs",
    "fetch:debug": "DATA_SOURCE=api node --inspect-brk --experimental-modules scripts/saveApiResponses.mjs",
    "dev:kill": "lsof -n -iTCP:3021 | sed '1 d' | awk '{print $2}' | xargs kill -9",
    "dev:debug": "DATA_SOURCE=cache PORT=3021 node --inspect-brk node_modules/.bin/nuxt",
    "develop": "DATA_SOURCE=cache PORT=3021 yarn dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "export": "nuxt export",
    "serve": "nuxt serve",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js"
  },
  "dependencies": {
    "@highcharts/map-collection": "^1.1.3",
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/redirect-module": "^0.3.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "@sendgrid/mail": "^7.4.0",
    "@thorchain/asgardex-util": "^0.3.0",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "bignumber.js": "^9.0.0",
    "date-fns": "^2.12.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "geoip-lite": "^1.4.2",
    "highcharts": "^8.1.2",
    "node-sass": "^4.14.1",
    "numeral": "^2.0.6",
    "nuxt": "^2.13.0",
    "nuxt-highcharts": "^1.0.1",
    "redis": "^3.0.2",
    "sass-loader": "^9.0.2",
    "sort-by": "^1.2.0"
  },
  "browser": {
    "fs": false
  },
  "devDependencies": {
  }
}
